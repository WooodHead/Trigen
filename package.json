{
  "name": "pass-creator-frontend",
  "version": "1.0.0",
  "description": "This is a react web frontend for passCreator",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server",
    "build": "NODE_ENV=production rimraf dist && webpack --config ./webpack/webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --coverage --watch",
    "coveralls": "NODE_ENV=test jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/on3iro/passCreator-Frontend.git"
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs"
          ]
        ]
      }
    },
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "keywords": [
    "frontend",
    "passCreator",
    "react",
    "redux"
  ],
  "author": "Theo Salzmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/on3iro/passCreator-Frontend/issues"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/global-styles.js",
      "!**/src/index.js",
      "!**/src/rootReducer.js",
      "!**/src/rootSaga.js",
      "!**/src/routes.js",
      "!**/src/store.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "homepage": "https://github.com/on3iro/passCreator-Frontend#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "css-loader": "^0.23.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "moxios": "^0.3.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "surge": "^0.19.0",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "grid-styled": "^1.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.14.3",
    "reselect": "^2.5.4",
    "sanitize.css": "^4.1.0",
    "shortid": "^2.2.8",
    "styled-components": "^1.4.3"
  }
}
